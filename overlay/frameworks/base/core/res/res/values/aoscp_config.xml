<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Wether device should show software navbar
        on first boot -->
    <bool name="config_defaultToNavigationBar">false</bool>

    <!-- Whether to intercept inputs on screen off. -->
    <bool name="config_enableKeyHandler">true</bool>
	
    <!-- Keypress Boost duration -->
    <integer name="fpunlockboost_duration">800</integer>
	
	<!-- Doze: should the TYPE_PICK_UP_GESTURE sensor be used as a pulse signal. -->
    <bool name="config_dozePulsePickup">true</bool>
	
</resources>